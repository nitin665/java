import java.utill.Scanner;
public class mark{
  public static void main (String args[]){
    Scanner sc=new Scanner(System.in);
    String name=sc.next();
    int a;
    a= sc.nextInt();
    int b=sc.nextInt();
    int c=sc.nextInt();
    int d=sc.nextInt();
    int e=a+b+c+d
     
    System.out.println("name "+name);
    System.out.println("rollno  "+a);
    System.out.println("total "+e);
  }
}